{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Current TS File",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/tsx/dist/cli.mjs",
      "args": ["${file}"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
